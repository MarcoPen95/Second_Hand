<!-- profile page -->
<div class="container">
<div class="columns">
   <div class="column is-2">
    </div>
   <div class="column is-5">
    <div class="card">
       <div class="card-image">
       <figure class="image is-3by3">
       <% if current_seller.avatar.attached? %>
       <a href='#'> <%= image_tag(current_seller.avatar ) %></a>
       <% end %>
      </figure>
  </div>
  <br>
  <br>
   <div id="features" class="card-content">
    <div class="content">
      <p>Username:<%= current_seller.username %> </p>
      <p>Email:<%= current_seller.email %> </p>
    </div>
  </div>
</div>
</div>
<br>
<br>
</div>
</div>
